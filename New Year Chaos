void minimumBribes(int q_count, int* q) {
    int bribes = 0;

    for (int i = q_count - 1; i >= 0; i--) {
        if (q[i] - (i + 1) > 2) {
            printf("Too chaotic\n");
            return;
        }

        // Only check from max(0, q[i] - 2) to i - 1
        for (int j = (q[i] - 2 > 0 ? q[i] - 2 : 0); j < i; j++) {
            if (q[j] > q[i]) {
                bribes++;
            }
        }
    }

    printf("%d\n", bribes);
}
