int flippingMatrix(vector<vector<int>> matrix) {
    int sum=0;
    int n=matrix.size()/2;
    for(int i=0; i<n ;i++){
        for(int j=0; j<n; j++){
            sum+=max(max(matrix[i][j], matrix[2*n-1-i][j]), max(matrix[i][2*n-1-j], matrix[2*n-i-1][2*n-1-j]));
        }
       
    }
     return sum;

}
